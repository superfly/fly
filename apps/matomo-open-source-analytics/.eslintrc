{
  "extends": [
    "eslint:recommended",
  ],
  "parser": "babel-eslint",
  "rules": {
    "react/no-multi-comp": 0,
    "react/no-find-dom-node": 0,
    "import/default": 0,
    "import/no-duplicates": 0,
    "import/named": 0,
    "import/namespace": 0,
    "import/no-unresolved": 0,
    "import/no-named-as-default": 0,
    "comma-dangle": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-console": 0,
    "no-alert": 0,
    "semi": 0,
    "space-before-function-paren": 0,
    "no-else-return": 0,
    "quotes": 0,
    "arrow-parens": 0,
    "no-const-assign": 0
  },
  "globals": {
    "socket": true,
    "webpackIsomorphicTools": true,
    "Promise": false,
    "Uint8Array": false,
    "fly": false,
    "fetch": false,
    "app": false,
    "Response": false,
    "Request": false,
    "Buffer": false,
    "URL": false,
    "URLSearchParams": false,
    "console": false
  }
}
