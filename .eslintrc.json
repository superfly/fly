{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "worker": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "no-const-assign": "warn",
    "no-this-before-super": "warn",
    "no-undef": "warn",
    "no-unreachable": "warn",
    "no-unused-vars": "warn",
    "constructor-super": "warn",
    "valid-typeof": "warn"
  },
  "globals": {
    "_ivm": true,
    "registerMiddleware": true,
    "fly": true
  }
}