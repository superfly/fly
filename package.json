  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**/*",
      "**/*.js",
      "v8env/"
    ],
    "reporter": [
      "text"
    ],
    "all": true
  }
