{
  "name": "@fly/cli",
  "version": "0.48.0-pre.0",
  "description": "fly cli",
  "homepage": "https://github.com/superfly/fly#readme",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "bin": {
    "fly": "./bin/run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/superfly/fly"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "clean": "rm -rf ./lib ./dist ./tmp"
  },
  "bugs": {
    "url": "https://github.com/superfly/fly/issues"
  },
  "dependencies": {
    "@fly/build": "0.47.1",
    "@fly/core": "0.48.0-pre.0",
    "@fly/examples": "0.47.1",
    "@fly/v8env": "0.47.1",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "axios": "^0.18.0",
    "chalk": "^2.4.2",
    "inquirer": "^6.2.2",
    "js-yaml": "^3.13.0",
    "pako": "1.0.6",
    "tar-fs": "^1.16.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/inquirer": "^0.0.44",
    "@types/node": "^10",
    "@types/pako": "^1.0.0",
    "@types/tar-fs": "^1.16.0",
    "globby": "^8",
    "ts-node": "^8",
    "tslint": "^5"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "fly",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "macos": {
      "identifier": "com.superfly.fly"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
